{
  "id": "repeaters-cluster-count",
  "type": "symbol",
  "source": "repeaters-source",
  "filter": [
    "all",
    ["has", "point_count"],
    ["!", ["has", "is_same_location_cluster"]]
  ],
  "maxzoom": 14,
  "layout": {
    "text-field": ["get", "point_count_abbreviated"],
    "text-font": ["DIN Pro Medium", "Arial Unicode MS Bold"],
    "text-size": 14,
    "text-allow-overlap": true
  },
  "paint": {
    "text-color": "#FFFFFF"
  }
}
